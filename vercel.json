{
    "buildCommand": "cd campus-bites && npm install && npm run build",
    "outputDirectory": "campus-bites/dist",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "routes": [
        {
            "src": "/[^.]+",
            "dest": "/",
            "status": 200
        }
    ]
}